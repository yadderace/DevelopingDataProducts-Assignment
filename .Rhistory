dfResult <- data.frame(
x = runif(20, min = 0, max = 100),
y = runif(20, min = 0, max = 100)
)
View(dfResult)
objCluster <- kmeans(dfResult, centers = 3)
clusters <- objCluster$cluster
clusters
fncFindHull <- function(dfTemp) dfTemp[chull(dfTemp$x, dfTemp$y), ]
dfResult$cluster <- clusters
dfResult
hulls <- ddply(dfResult, "cluster", fncFindHull)
View(hulls)
